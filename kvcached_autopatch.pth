try:
    import os
    if os.getenv("ENABLE_KVCACHED", "false").lower() in ("true", "1"):
        import importlib
        os.environ.setdefault("KVCACHED_AUTOPATCH", "1")
        mod = importlib.import_module("kvcached.autopatch")
        getattr(mod, "autopatch_all", lambda: None)()
except ImportError:
    pass
